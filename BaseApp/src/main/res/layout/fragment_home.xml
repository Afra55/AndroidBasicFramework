<com.afra55.baseclient.view.refreshforheader.PtrFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cube_ptr="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_pull_to_refresh"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    cube_ptr:ptr_duration_to_close="200"
    cube_ptr:ptr_duration_to_close_header="1000"
    cube_ptr:ptr_keep_header_when_refresh="true"
    cube_ptr:ptr_pull_to_fresh="false"
    cube_ptr:ptr_ratio_of_header_height_to_refresh="1.2"
    cube_ptr:ptr_resistance="1.7"
    tools:context=".module.home.HomeFragment">

    <RelativeLayout
        android:id="@+id/home_container_rly"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include android:id="@+id/home_binner" layout="@layout/layout_binner" />

    </RelativeLayout>


</com.afra55.baseclient.view.refreshforheader.PtrFrameLayout>
